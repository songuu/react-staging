{
  "name": "create-stage-template",
  "version": "2.0.2",
  "description": "基于pnpm+monorepo+rollup开发的前端脚手架",
  "scripts": {
    "commit": "git-cz && git push",
    "add": "pnpm changeset add",
    "push": "pnpm changeset publish",
    "build:stage-cli": "pnpm --filter create-stage-template build"
  },
  "engines": {
    "node": ">=14.14"
  },
  "dependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@types/eslint": "^8.56.6",
    "@types/validate-npm-package-name": "^4.0.2",
    "commitizen": "^4.3.0",
    "@songyulala/create-eslint-config": "workspace:^",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.9.0",
    "typescript": "^5.4.2",
    "validate-npm-package-name": "^5.0.0"
  },
  "keywords": [
    "staging",
    "frontEnd",
    "前端脚手架",
    "react"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1"
  },
  "files": ["packages/core"]
}