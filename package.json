{
  "name": "staging",
  "version": "1.0.0",
  "description": "基于pnpm+monorepo+rollup开发的前端脚手架",
  "scripts": {
    "commit": "git-cz && git push",
    "add": "pnpm changeset add",
    "push": "pnpm changeset publish",
    "build:stage-utils": "pnpm --filter stage-utils build",
    "build:stage-cli": "pnpm --filter core build"
  },
  "engines": {
    "node": ">=14.14"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.8.0",
    "typescript": "^5.3.3",
    "validate-npm-package-name": "^5.0.0",
    "@types/validate-npm-package-name": "^4.0.2"
  },
  "keywords": [
    "staging",
    "frontEnd",
    "前端脚手架",
    "react"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}