{
  "name": "staging",
  "version": "1.0.0",
  "description": "frontEnd staging",
  "scripts": {
    "commit": "git-cz && git push",
    "add": "pnpm changeset add",
    "push": "pnpm changeset publish",
    "build:staging-cli": "pnpm --filter staging-mode build",
    "build:staging-utils": "pnpm --filter staging-utils build"
  },
  "engines": {
    "node": ">=14.14"
  },
  "dependencies": {
    "@changesets/cli": "^2.26.0",
    "commitizen": "^4.3.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "cz-conventional-changelog": "^3.3.0",
    "cz-git": "^1.6.1",
    "typescript": "^4.9.5"
  },
  "keywords": ["staging", "frontEnd"],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}